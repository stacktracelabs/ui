{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": ["src-js/**/*", "src-js/**/*.vue"],
  "exclude": ["src-js/**/__tests__/*"],
  "compilerOptions": {
    "skipLibCheck": true,
    "composite": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src-js/*"]
    }
  },
  "references": [
    {
      "path": "./tsconfig.node.json"
    }
  ]
}
